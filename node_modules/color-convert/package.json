{
  "name": "color-convert",
  "description": "Plain color conversion functions",

  "version": "1.9.3",

  "version": "2.0.1",

  "author": "Heather Arthur <fayearthur@gmail.com>",
  "license": "MIT",
  "repository": "Qix-/color-convert",
  "scripts": {
